  "scripts": {
    "build": "npm run front:build && npm run back:build",
    "start": "NODE_ENV=production node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently -c red,blue -n front,back \"npm run front:dev\" \"npm run back:dev\"",
    "watch": "npm run back:tsc",
    "front:dev": "node server.js",
    "front:build": "next build frontend",
    "back:tsc": "tsc -p backend -w",
    "back:dev": "nodemon backend/build/scripts/runServer --watch backend/build",
    "back:build": "tsc -p backend"
  },
